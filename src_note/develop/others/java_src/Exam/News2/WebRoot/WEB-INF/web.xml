<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>QuerySevlet</servlet-name>
    <servlet-class>news.servlet.test.QuerySevlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PageControlServlet</servlet-name>
    <servlet-class>news.servlet.test.PageControlServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>QuerySevlet</servlet-name>
    <url-pattern>QuerySevlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PageControlServlet</servlet-name>
    <url-pattern>/PageControlServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
