<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QueryStudentInfo</servlet-name>
    <servlet-class>accp.test.servlet.QueryStudentInfo</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>QueryStuSerlvetID</servlet-name>
    <servlet-class>accp.test.servlet.QueryStuSerlvetID</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>accp.test.servlet.UpdateServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>QueryStudentInfo</servlet-name>
    <url-pattern>/QueryStudentInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryStuSerlvetID</servlet-name>
    <url-pattern>/QueryStuSerlvetID</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateServlet</servlet-name>
    <url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>

</web-app>