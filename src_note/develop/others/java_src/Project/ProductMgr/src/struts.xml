<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	<!-- <include file="/WEB-INF/dtd/..xml"></include> -->
	<package name="product" namespace="jsp" extends="struts-default">
		<action name="*action" class="product.action.ProductAction" method="{1}">
			<result name="input" type="">/WEB-INF/jsp/{1}login.jsp</result>
			<result name="success">/WEB-INF/{1}product-list.jsp</result>
		</action>
	</package>
</struts>    
