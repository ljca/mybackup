

PS1="\u:\w\n\\$ "

HISTCONTROL="ignoreboth"

PATH="$HOME/bin:$PATH"

export PS1 HISTCONTROL PATH

alias l="ls -alSh --color=auto"

alias du="du -h"

alias df="df -h"
alias dirs="dirs -v"

alias pkgi="pkg install"
alias pkgr="pkg uninstall"
alias pkgs="pkg search"

alias al="proot-distro login archlinux"
