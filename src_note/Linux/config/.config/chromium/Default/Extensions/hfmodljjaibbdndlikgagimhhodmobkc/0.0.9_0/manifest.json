{
   "background": {
      "persistent": false,
      "scripts": [ "promisify.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "mht.png"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+S"
         }
      }
   },
   "description": "Save webpage as single .mht file.",
   "homepage_url": "https://github.com/vsDizzy/SaveAsMHT",
   "icons": {
      "128": "mht.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Save As MHT",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "contextMenus", "downloads", "pageCapture", "storage", "tabs", "file:///*" ],
   "short_name": "SaveAsMHT",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.9"
}
