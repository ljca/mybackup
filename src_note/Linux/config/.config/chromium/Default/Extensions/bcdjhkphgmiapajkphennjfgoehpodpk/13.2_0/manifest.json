{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icon16.png"
      },
      "default_popup": "popup.html",
      "default_title": "Zoom Page WE"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Show Zoom Popup",
         "suggested_key": {
            "default": "Alt+Z"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   } ],
   "description": "Zoom web pages (either per-site or per-tab) using full-page zoom, text-only zoom, automatic fit-to-width and minimum font size.",
   "icons": {
      "128": "icon128-96.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "Zoom Page WE",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "webNavigation", "contextMenus", "notifications", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "13.2"
}
